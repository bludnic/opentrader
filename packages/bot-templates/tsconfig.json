{
  "extends": "@opentrader/tsconfig/esm.json",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "../../dts/packages/bot-templates",
    "paths": {
      "@opentrader/bot-processor": [
        "../bot-processor/src/index.ts"
      ],
      "@opentrader/bot-processor/*": [
        "../bot-processor/src/*"
      ],
      "@opentrader/eslint/*": [
        "../eslint/*"
      ],
      "@opentrader/exchanges": [
        "../exchanges/src/index.ts"
      ],
      "@opentrader/exchanges/*": [
        "../exchanges/src/*"
      ],
      "@opentrader/logger": [
        "../logger/src/index.ts"
      ],
      "@opentrader/logger/*": [
        "../logger/src/*"
      ],
      "@opentrader/tools": [
        "../tools/src/index.ts"
      ],
      "@opentrader/tools/*": [
        "../tools/src/*"
      ],
      "@opentrader/tsconfig/*": [
        "../tsconfig/*"
      ],
      "@opentrader/types": [
        "../types/src/index.ts"
      ],
      "@opentrader/types/*": [
        "../types/src/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ],
  "references": [
    {
      "path": "../bot-processor"
    },
    {
      "path": "../eslint"
    },
    {
      "path": "../exchanges"
    },
    {
      "path": "../logger"
    },
    {
      "path": "../tools"
    },
    {
      "path": "../tsconfig"
    },
    {
      "path": "../types"
    }
  ]
}
